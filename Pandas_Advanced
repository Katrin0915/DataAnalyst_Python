      },
      "outputs": [],
      "source": [
        "log.loc[:, 'source_type'] = log.traffic_source\n",
        "log.loc[(log.traffic_source == 'yandex')|(log.traffic_source == 'google'), 'source_type'] = 'organic'\n",
        "log.loc[(log.region == 'Russia')&((log.traffic_source =='paid')|(log.traffic_source =='email')), 'source_type'] = 'ad'\n",
        "log.loc[(log.region != 'Russia')&((log.traffic_source =='paid')|(log.traffic_source =='email')), 'source_type'] = 'other'"
      ]
